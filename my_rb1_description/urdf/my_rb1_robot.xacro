<?xml version="1.0" ?>

<robot name="robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:include filename="$(find my_rb1_description)/urdf/my_rb1_robot.gazebo" />

    <!-- Parameters -->
    <!--<xacro:property name="chassis_mass" value="10" />
    <xacro:property name="pi" value="3.1415926535897931"/>-->

    	    <!-- Link - base_footprint -->
    <link name="base_footprint">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0.5" rpy="0 0 0" />
            <inertia ixx="0.37505" ixy="0" ixz="0" iyy="0.37505" iyz="0" izz="0.125" />
        </inertial>

        <collision>
            <geometry>
                <cylinder length="0.01" radius="0.25"/>
            </geometry>
            <!--<geometry>
                <mesh filename="package://robot_description/meshes/chassis.stl" />
            </geometry>-->
        </collision>

        <visual>
            <geometry>
                <cylinder length="0.01" radius="0.25"/>
            </geometry>
            <!-- <geometry>
                <box size="2 1.3 0.4" />
            </geometry> -->
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>
    </link>

     <!-- Joint - base_link -->
    <joint name="joint_footprint_base_link" type="fixed">
        <origin rpy="0 0 0" xyz="0 0 0.15" />
        <child link="base_link" />
        <parent link="base_footprint" />
    </joint>

        <!-- Link - base_link -->
    <link name="base_link">
        <inertial>
            <mass value="19" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.677" ixy="0" ixz="0" iyy="0.677" iyz="0" izz="0.59375" />
        </inertial>

        <collision>
            <geometry>
                <cylinder length="0.29" radius="0.25"/>
            </geometry>
            <!--<geometry>
                <mesh filename="package://robot_description/meshes/chassis.stl" />
            </geometry>-->
        </collision>

        <visual>
             <geometry>
                <cylinder length="0.29" radius="0.25"/>
            </geometry>
            <!-- <geometry>
                <box size="2 1.3 0.4" />
            </geometry> -->
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>
    </link>

         <!-- Joint - base_link / left wheel -->
    <joint name="joint_base_link_left_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 -0.2 -0.15" />
        <child link="left_wheel" />
        <parent link="base_link" />
        <!--<axis rpy="0 0 0" xyz="0 1 0" />-->
    </joint>

      <!-- Link - link_left_wheel -->
    <link name="left_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 -0.2 0.5" rpy="1.5707 0 0" />
            <inertia ixx="0.0002083" ixy="0" ixz="0" iyy="0.0003125" iyz="0" izz="0.0002083" />
        </inertial>

        <collision>
            <geometry>
                <cylinder length="0.025" radius="0.025"/>
            </geometry>
            <!--<geometry>
                <mesh filename="package://robot_description/meshes/chassis.stl" />
            </geometry>-->
        </collision>

        <visual>
         <origin xyz="0 0 0" rpy="1.5707 0 0" />
             <geometry>
                <cylinder length="0.025" radius="0.025"/>
            </geometry>
            <!-- <geometry>
                <box size="2 1.3 0.4" />
            </geometry> -->
            <!--<material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>-->
        </visual>
    </link>
            <!-- Joint - base_link / right wheel -->
    <joint name="joint_base_link_right_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 0.2 -0.15" />
        <child link="right_wheel" />
        <parent link="base_link" />
        <!--<axis rpy="0 0 0" xyz="0 1 0" />-->
    </joint>

          <!-- Link - link_right_wheel -->
    <link name="right_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0.2 0.5" rpy="1.5707 0 0" />
            <inertia ixx="0.0002083" ixy="0" ixz="0" iyy="0.0003125" iyz="0" izz="0.0002083" />
        </inertial>

        <collision>
            <geometry>
                <cylinder length="0.025" radius="0.025"/>
            </geometry>
            <!--<geometry>
                <mesh filename="package://robot_description/meshes/chassis.stl" />
            </geometry>-->
        </collision>

        <visual>
         <origin xyz="0 0 0" rpy="1.5707 0 0" />
             <geometry>
                <cylinder length="0.025" radius="0.025"/>
            </geometry>
            <!-- <geometry>
                <box size="2 1.3 0.4" />
            </geometry> -->
            <!--<material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>-->
        </visual>
    </link>

             <!-- Joint - base_link / front caster wheel -->
    <joint name="joint_base_link_front_caster_wheel" type="fixed">
        <origin rpy="0 0 0" xyz="0.1 0.0 -0.15" />
        <child link="front_caster" />
        <parent link="base_link" />
    </joint>

     <!-- Link - front_caster_wheel -->
    <link name="front_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="0.1 0.0 0.5" rpy="0 0 0" />
            <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025" iyz="0" izz="0.00025" />
        </inertial>

        <collision>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <!--<geometry>
                <mesh filename="package://robot_description/meshes/chassis.stl" />
            </geometry>-->
        </collision>

        <visual>
             <geometry>
                 <sphere radius="0.025"/>
            </geometry>
            <!-- <geometry>
                <box size="2 1.3 0.4" />
            </geometry> -->
           <material name="dark_grey">
            <color rgba="0.662745 0.662745 0.662745 1.0"/>
            </material>
        </visual>
    </link>

        <!-- Joint - base_link / back caster wheel -->
        <joint name="joint_base_link_back_caster_wheel" type="fixed">
        <origin rpy="0 0 0" xyz="-0.1 0.0 -0.15" />
        <child link="back_caster" />
        <parent link="base_link" />
    </joint>

         <!-- Link - back_caster_wheel -->
    <link name="back_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="-0.1 0.0 0.5" rpy="0 0 0" />
            <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025" iyz="0" izz="0.00025" />
        </inertial>

        <collision>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <!--<geometry>
                <mesh filename="package://robot_description/meshes/chassis.stl" />
            </geometry>-->
        </collision>

        <visual>
             <geometry>
                 <sphere radius="0.025"/>
            </geometry>
            <!-- <geometry>
                <box size="2 1.3 0.4" />
            </geometry> -->
            <material name="dark_grey">
                <color rgba="0.662745 0.662745 0.662745 1.0"/>
            </material>
        </visual>
    </link>

        <!-- Laser scan -->
    <joint name="joint_front_laser_base_link" type="fixed">
        <origin rpy="3.14 0 0" xyz="0.25 0.0 0.075"/>
        <child link="front_laser" />
        <parent link="base_link" />
    </joint>
    <link name="front_laser">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="0.000133" ixy="0" ixz="0" iyy="0.000133" iyz="0" izz="0.0002"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.02" length="0.02"/>
            </geometry>
            <material name="Red">
                <color rgba="0.7 0.1 0.1 1" />
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.02" length="0.02"/>
            </geometry>
        </collision>
    </link>
</robot>